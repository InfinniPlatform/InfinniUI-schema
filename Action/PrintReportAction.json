{
  "id": "https://raw.githubusercontent.com/InfinniPlatform/InfinniUI-schema/master/Action/PrintReportAction.json",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Действие открытия отчета",
  "type": "object",
  "allOf": [
    {
      "$ref": "https://raw.githubusercontent.com/InfinniPlatform/InfinniUI-schema/master/Action/ActionBase.json"
    }
  ],
  "properties": {
    "ConfigId": {
      "description": "Идентификатор конфигурации",
      "type": "string"
    },
    "ReportId": {
      "description": "Идентификатор отчета",
      "type": "string"
    },
    "Parameters": {
      "description": "Список значений параметров отчета",
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "Name": {
            "description": "Наименование параметра отчета",
            "type": "string"
          },
          "Value": {
            "description": "Привязка данных текущего представления на параметр отчета",
            "$ref": "https://raw.githubusercontent.com/InfinniPlatform/InfinniUI-schema/master/DataBinding/DataBinding.json"
          }
        },
        "required": [
          "Name",
          "Value"
        ]
      }
    }
  },
  "required": [
    "ConfigId",
    "ReportId"
  ]
}