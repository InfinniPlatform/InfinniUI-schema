{
  "id": "https://raw.githubusercontent.com/InfinniPlatform/InfinniUI-schema/master/Components/DocumentViewer/DocumentViewer.json",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Элемент для отображения печатного представления",
  "type": "object",
  "allOf": [
    {
      "$ref": "https://raw.githubusercontent.com/InfinniPlatform/InfinniUI-schema/master/Element/Element.json"
    }
  ],
  "properties": {
    "PrintViewId": {
      "description": "Идентификатор печатного представления",
      "type": "string"
    },
    "PrintViewType": {
      "description": "Тип печатного представления",
      "type": "string",
      "default": "ObjectView"
    },
    "Source": {
      "description": "Привязка данных для источника",
      "$ref": "https://raw.githubusercontent.com/InfinniPlatform/InfinniUI-schema/master/DataBinding/DataBinding.json"
    }
  },
  "required": [
    "ConfigId",
    "DocumentId"
  ]
}