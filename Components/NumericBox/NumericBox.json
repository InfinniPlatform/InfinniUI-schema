{
  "id": "https://raw.githubusercontent.com/InfinniPlatform/InfinniUI-schema/master/Components/NumericBox/NumericBox.json",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Редактор чисел",
  "type": "object",
  "allOf": [
    {
      "$ref": "https://raw.githubusercontent.com/InfinniPlatform/InfinniUI-schema/master/Components/TextEditorBase/TextEditorBase.json"
    }
  ],
  "properties": {
    "MinValue": {
      "description": "Минимальное значение",
      "type": "number"
    },
    "MaxValue": {
      "description": "Максимальное значение",
      "type": "number"
    },
    "Increment": {
      "description": "Приращение значения",
      "type": "number",
      "default": 1
    },
    "StartValue": {
      "description": "Начальное значение по умолчанию",
      "type": "number"
    }
  }
}